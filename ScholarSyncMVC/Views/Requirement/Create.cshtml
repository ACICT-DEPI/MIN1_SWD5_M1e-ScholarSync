@model RequirementVM
@{
    ViewData["Title"] = "Create";
}
@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="m-auto w-75  bg-white p-5">
    <h1>Create New Scholarship Requirement</h1>
    @if (!string.IsNullOrEmpty(TempData["Message"] as string))
    {
        <div class="alert alert-primary mt-3 mb-2">
            @TempData["message"]
        </div>
    }
    <hr />
<form asp-action="Create" method="post">
   
    <partial name="PartialView/_CreateEdit" />
    <div class="form-group mt-4 text-center">
        <input type="submit" value="Create" class="btn btn-primary btn-lg" />
        <a asp-action="Index" class="btn btn-light btn-lg">Back</a>
    </div>
</form>
</div>
@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
